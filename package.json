{
  "name": "renew-ip",
  "version": "1.1.0",
  "description": "A module for renewing 4G/LTE IP addresses on tethered Android phones from Node.",
  "keywords": [
    "4g",
    "lte",
    "tethering",
    "ip-renewal",
    "automate"
  ],
  "license": "MIT",
  "author": "0x00trace",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "bin"
  ],
  "bin": {
    "renew-ip": "./bin/renew"
  },
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "depcheck": "dependency-check ./package.json --verbose --ignore-module \"@types/*\" --nodev",
    "lint": "eslint . --ext .ts --fix",
    "pub": "npm run build && npm publish",
    "sort": "sort-package-json",
    "preinstall": "npx only-allow pnpm",
    "test": "nyc --extension .ts mocha --forbid-only --comp test/**/*.test.ts",
    "upgrade": "ncu -u --packageFile package.json && run-s sort && run-s update"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "dotenv": "^8.6.0",
    "got": "^11.8.2",
    "is-online": "^9.0.1",
    "tslib": "^2.3.1",
    "zod": "^3.10.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "^2.4.3",
    "@types/debug": "^4.1.7",
    "@types/node": "^14.17.27",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "~7.32.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "sort-package-json": "^1.52.0",
    "typescript": "4.2.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "engines": {
    "node": ">=14"
  }
}
